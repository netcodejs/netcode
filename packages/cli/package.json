{
  "name": "@netcodejs/cli",
  "version": "0.0.1",
  "author": "moruiyi @mooooooi",
  "bin": {
    "netcode-cli": "./bin/run"
  },
  "bugs": "https://github.com/netcodejs/netcode/issues",
  "dependencies": {
    "@netcodejs/iser": "^0.0.1",
    "@netcodejs/util": "^0.0.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "esbuild": "^0.12.22",
    "esbuild-plugin-type-schema": "^1.0.5",
    "fast-glob": "^3.2.7",
    "ts-morph": "^11.0.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "globby": "^10",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
