{
  "targets": {
    "debug": {
      "binaryFile": "build/untouched.wasm",
      "textFile": "build/untouched.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "binaryFile": "build/optimized.wasm",
      "textFile": "build/optimized.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    },
    "test": {
      "binaryFile": "build/test.wasm",
      "textFile": "build/test.wat",
      "sourceMap": true,
      "debug": true
    }
  },
  "options": {
    "transform": ["./transform.ts"]
  }
}